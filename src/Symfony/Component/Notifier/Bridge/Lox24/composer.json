{
    "name": "symfony/lox24-notifier",
    "type": "symfony-notifier-bridge",
    "description": "Symfony LOX24 Notifier Bridge",
    "keywords": [
        "sms",
        "lox24",
        "notifier"
    ],
    "homepage": "https://symfony.com",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/notifier": "^7.2"
    },
    "require-dev": {
        "symfony/webhook": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\Component\\Notifier\\Bridge\\Lox24\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "php-cs-fixer": "php-cs-fixer --config=./.php_cs"
    },
    "minimum-stability": "dev"
}
